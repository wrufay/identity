{
  "expo": {
    "name": "ᯓ★ ProjectOrigin",
    "slug": "heritage-ar",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/projectorigin.png",
    "scheme": "heritagear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffdac6",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.heritagear"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/projectorigin.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffdac6",
          "dark": {
            "backgroundColor": "#ffdac6"
          }
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
